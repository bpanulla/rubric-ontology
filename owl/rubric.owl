        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AnalyticRubric">
    <rdfs:subClassOf rdf:resource="#Rubric"/>
  </owl:Class>
  <owl:Class rdf:ID="Category">

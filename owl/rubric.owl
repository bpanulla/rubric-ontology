  </owl:Class>
  <owl:Class rdf:about="#Scope">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <xsd:string rdf:ID="criterion">
    <rdfs:domain rdf:resource="#Element"/>
    <rdfs:subClassOf>
      <xsd:string rdf:ID="StringDatatypeProperty">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
      </xsd:string>
    </rdfs:subClassOf>
  </xsd:string>
  <Scope rdf:ID="Self"/>

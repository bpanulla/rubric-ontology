      </xsd:integer>
    </rdfs:subClassOf>
  </xsd:integer>
  <Scope rdf:ID="Team"/>
  <Scope rdf:ID="Individual"/>
  <xsd:string rdf:ID="benchmark">
    <rdfs:domain rdf:resource="#Element"/>
    <rdfs:subClassOf rdf:resource="#StringDatatypeProperty"/>
  </xsd:string>
  <xsd:string rdf:ID="levelFeedback">
    <rdfs:domain rdf:resource="#Level"/>
    <rdfs:subClassOf rdf:resource="#feedback"/>
  </xsd:string>
  <xsd:ID rdf:ID="key">
    <rdfs:subClassOf>
      <xsd:ID rdf:ID="GUIDDatatypeProperty">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
      </xsd:ID>
    </rdfs:subClassOf>
  </xsd:ID>
  <xsd:string rdf:ID="title">
    <rdfs:domain rdf:resource="#Rubric"/>
    <rdfs:subClassOf rdf:resource="#StringDatatypeProperty"/>
  </xsd:string>
</rdf:RDF>

